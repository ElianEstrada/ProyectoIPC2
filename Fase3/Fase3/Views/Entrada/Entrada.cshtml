@model LinkedList<Entidad.Proveedor>
@{
    ViewBag.Title = "Entrada";
    Layout = "~/Views/Layauts/BarraNavegacion.cshtml";
}

<h2>Entrada</h2>


<div class="row">
    <div class="col m12">
        <div class="card z-depth-3">
            <div class="card-content">
                <span class="card-title"> Registrar Entrada </span>
                <form>
                    <div class="row">
                        <div class="col m6">
                            Código Entrada:
                            <input type="number" step="0.01" placeholder="Código Entrada" />
                            Proveedor:
                            <select>
                                <option selected disabled>Elija Su Opción</option>
                                @foreach (var item in Model)
                                {
                                    <option> @item.nit, @item.nombre </option>
                                }
                            </select>
                        </div>
                        <div class="col m6">
                            Fecha Entrada: 
                            <input type="date"/>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
